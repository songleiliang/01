/* components/MyHeader/myheader.wxss */
.my-header {
   display: flex;
   justify-content: center;
   align-items: center;
   padding: 10rpx;
   background-color: #fff;
}
.logo{
   flex: 7;
}
.search {
   flex: 1;
   display: flex;
   justify-content: center;
   align-items: center;
}
.user-icon{
   flex: 2;
   display: flex;
   justify-content: center;
   align-items: center;
}
.download{
   flex: 2;
   font-size: 30rpx;
   display: flex;
   justify-content: center;
   align-items: center;
   background-color: pink;
   color: #fff;
   border-radius: 10rpx;
   padding: 10rpx;
}